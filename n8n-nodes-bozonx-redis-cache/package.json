{
    "name": "n8n-nodes-bozonx-redis-cache",
    "version": "1.0.0",
    "description": "n8n node for Redis cache",
    "license": "MIT",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Ivan K"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bozonx/ivan-k-automation-tools"
    },
   "scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
		"release:n8n": "n8n-node release",
		"prepublishOnly:n8n": "n8n-node prerelease"
	},
    "files": [
        "dist"
    ],
    "n8n": {
      "n8nNodesApiVersion": 1,
      "strict": true,
      "nodes": [
          "dist/nodes/RedisCache/BozonxRedisCache.node.js"
      ],
      "credentials": [
          "dist/credentials/Redis.credentials.js"
      ]
  },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "redis": "^5.0.0"
    }
}
