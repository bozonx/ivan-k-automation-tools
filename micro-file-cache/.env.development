# Основные настройки приложения для разработки
NODE_ENV=development
LISTEN_HOST=localhost
LISTEN_PORT=3000

# Аутентификация
# Включить аутентификацию для разработки
AUTH_ENABLED=true
# Секретный ключ для разработки (в продакшене используйте более сложный ключ)
AUTH_SECRET_KEY=dev-secret-key-for-micro-file-cache-12345678901234567890

# Пути к хранилищу файлов и метаданных
STORAGE_PATH=../test-data/micro-file-cache/storage

# Ограничения загрузки файлов
MAX_FILE_SIZE=104857600

# Разрешенные MIME типы для разработки
ALLOWED_MIME_TYPES=["image/jpeg","image/png","image/gif","image/webp","application/pdf","text/plain","application/json","text/csv","application/zip","application/x-zip-compressed"]

# Формат организации файлов по датам
DATE_FORMAT=YYYY-MM

# Включить дедупликацию файлов
ENABLE_DEDUPLICATION=true

# TTL настройки (в секундах)
MAX_TTL=86400
MIN_TTL=60
DEFAULT_TTL=3600

# Настройки автоматической очистки
CLEANUP_ENABLED=true
CLEANUP_CRON=0 * * * * *
CLEANUP_INTERVAL=60000

# Настройки логирования для разработки
LOG_LEVEL=debug

# CORS настройки для разработки
CORS_ORIGIN=true

# Дополнительные настройки для разработки
# Максимальное количество файлов в кэше
MAX_FILES_COUNT=10000

# Максимальный общий размер хранилища в байтах (1GB)
MAX_STORAGE_SIZE=1073741824
