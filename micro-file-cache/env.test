# Настройки приложения для тестирования
NODE_ENV=test
LISTEN_HOST=localhost
LISTEN_PORT=0

# Аутентификация для тестов
# Включить аутентификацию для тестов
AUTH_ENABLED=true
# Простой тестовый ключ для предсказуемых результатов
AUTH_SECRET_KEY=test-secret-key

# Пути к хранилищу файлов (будет переопределено в тестах)
STORAGE_PATH=../test-data/micro-file-cache/storage

# Ограничения загрузки файлов для тестов
MAX_FILE_SIZE=52428800

# Разрешенные MIME типы для тестов (расширенный список)
ALLOWED_MIME_TYPES=["image/jpeg","image/png","image/gif","image/webp","application/pdf","text/plain","application/json","text/csv","application/zip","application/x-zip-compressed","text/html","text/css","application/javascript","application/octet-stream"]

# Формат организации файлов по датам
DATE_FORMAT=YYYY-MM

# Включить дедупликацию файлов для тестов
ENABLE_DEDUPLICATION=true

# TTL настройки для тестов (в секундах)
MAX_TTL=86400
MIN_TTL=60
DEFAULT_TTL=3600

# Настройки автоматической очистки для тестов
CLEANUP_ENABLED=false
CLEANUP_CRON=0 * * * * *
CLEANUP_INTERVAL=60000

# Настройки логирования для тестов (минимальное логирование)
LOG_LEVEL=error

# CORS настройки для тестов
CORS_ORIGIN=true

# Дополнительные настройки для тестов
# Максимальное количество файлов в кэше
MAX_FILES_COUNT=1000

# Максимальный общий размер хранилища в байтах (100MB для тестов)
MAX_STORAGE_SIZE=104857600

# API настройки
API_PREFIX=api
API_VERSION=v1
