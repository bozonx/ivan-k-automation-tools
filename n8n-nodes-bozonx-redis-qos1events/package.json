{
    "name": "n8n-nodes-bozonx-redis-qos1events",
    "version": "0.5.0",
    "description": "n8n node for Redis QoS1 events",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Ivan",
        "email": "ipkozyrin@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bozonx/ivan-k-automation-tools"
    },
    "scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
		"release:n8n": "n8n-node release",
		"prepublishOnly:n8n": "n8n-node prerelease"
